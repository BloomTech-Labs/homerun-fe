    "styled-components": "^5.1.0",
    "yup": "^0.29.1"
  },
  "watch": {
    "build:css": "src/components/**/*.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\"",
    "start": "react-scripts start",
    "watch": "npm-watch",
    "build": "npm run build:css && node --max_old_space_size=2560 node_modules/.bin/react-scripts build",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "CI=true react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "autoprefixer": "^9.8.0",
    "fetch-mock": "^9.4.0",
    "jest-canvas-mock": "^2.2.0",
    "moxios": "^0.4.0",
    "postcss-cli": "^7.1.1",
    "redux-mock-store": "^1.5.4",
    "tailwindcss": "^1.4.6",
    "tailwindcss-inset": "^1.0.0",
    "ts-jest": "^26.1.0"
  }
}
